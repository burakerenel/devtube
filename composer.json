{
    "name": "burakerenel/devtube",
    "description": "A Laravel package for downloading videos from the net by simply passing a URL",
    "type": "library",
    "version": "2.0.6",
    "require": {
        "masih/youtubedownloader":"dev-master",
        "athlon1600/youtube-downloader": "dev-master",
        "norkunas/youtube-dl-php": "dev-master"
    },
    "require-dev": {
        "vlucas/phpdotenv": "dev-master",
        "orchestra/testbench": "dev-master"
    },
    "keywords": [
        "devtube",
        "devswebdev",
        "laravel",
        "downloader",
        "youtube-dl",
        "laravel-package",
        "mp4",
        "mp3",
        "video downloader",
        "mp3 downloader",
        "media downloader",
        "youtube-downloader",
        "laravel downloader",
        "laravel youtube downloader"
    ],
    "license": "MIT",
    "authors": [{
        "name": "DevinNorgarb",
        "email": "dnorgarb@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "DevsWebDev\\DevTube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevsWebDev\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevsWebDev\\DevTube\\DevTubeServiceProvider"
            ]
        }
    }
}
